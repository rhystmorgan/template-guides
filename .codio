{
  // This file is used to configure the three buttons along Codio's top menu.
  // Run button configuration
  "commands": {
    "Run Node": "node {{filepath}}"
  },
  // Preview button configuration
  "preview": {
    "Preview Website (static)": "https://{{domain}}/{{index}}",
    "Preview Current File (static)": "https://{{domain}}/{{filepath}}"
  },
  // Debugger target button configuration
  "debugger": [
    {
      "type": "NODEJS",
      "command": "{{filepath}}",
      "before": "",
      "uuid": "",
      "name": "Debug Current NodeJS File"
    }
  ]
}